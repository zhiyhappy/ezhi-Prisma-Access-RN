        <entry name="{{ rn_name }}">
          <protocol>
            <bgp>
              <enable>no</enable>
            </bgp>
          </protocol>
          <subnets>
            <member>{{ rn_subnet }}</member>
          </subnets>
          <region>{{ rn_region }}</region>
          <license-type>{{ rn_license_type }}</license-type>
          <ipsec-tunnel>{{ ipsec_tun_name }}</ipsec-tunnel>
          <secondary-wan-enabled>no</secondary-wan-enabled>
        </entry>